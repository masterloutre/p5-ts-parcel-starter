{
    "name": "p5-ts-bootstrap",
    "version": "1.0.0",
    "description": "Starter project for ts project with p5js, serving on local server",
    "dependencies": {
        "p5": "^1.4.1"
    },
    "devDependencies": {
        "@types/p5": "^0.9.0",
        "browser-sync": "^2.27.10",
        "parcel": "^2.5.0",
        "typescript": "^4.6.4"
    },
    "scripts": {
        "build-tsc": "npx tsc",
        "start": "parcel src/index.html --open",
        "build": "parcel build src/index.html",
        "watch": "parcel watch src/index.html",
        "serve-gitpod": "browser-sync dist -f dist/*.html -f dist/*.js --no-open --no-ui --listen 0.0.0.0"
    },
    "keywords": [
        "p5js",
        "p5.js",
        "processing",
        "p5 typescript"
    ]
}
